{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"_artifactsLocation": {
			"value": "https://raw.githubusercontent.com/ccuz/openshift-origin/bugfix/release-3.9-bugfixes"
		},
		"masterVmSize": {
			"value": "Standard_D4s_v3"
		},
		"infraVmSize": {
			"value": "Standard_DS2_v2"
		},
		"nodeVmSize": {
			"value": "Standard_D2s_v3"
		},
		"storageKind": {
			"value": "managed"
		},
		"openshiftClusterPrefix": {
			"value": "changeme"
		},
		"masterInstanceCount": {
			"value": 1
		},
		"infraInstanceCount": {
			"value": 1
		},
		"nodeInstanceCount": {
			"value": 1
		},
		"dataDiskSize": {
			"value": 64
		},
		"adminUsername": {
			"value": "changeme"
		},
		"openshiftPassword": {
			"value": "changeme"
		},
		"enableMetrics": {
			"value": "false"
		},
		"enableLogging": {
			"value": "false"
		},
		"sshPublicKey": {
			"value": "changeme" # SSH public key matching what you put in the keyvault
		},
		"keyVaultResourceGroup": {
			"value": "KeyvaultRG"
		},
		"keyVaultName": {
			"value": "changeme" # Add the name of your Key Vault here
		},
		"keyVaultSecret": {
			"value": "changeme" # Add the name of your Key Vault Secret here
		},
		"enableAzure": {
			"value": "true"
		},
		"aadClientId": {
			"value": "changeme" # Add the AppId you saved earlier
		},
		"aadClientSecret": {
			"value": "changeme" # Add the password for your Service Principal
		},
		"defaultSubDomainType": {
			"value": "nipio"
		},
		"defaultSubDomain": {
			"value": "changeme" # If you selected nipio above, this will be ignored
		}
	}
}
